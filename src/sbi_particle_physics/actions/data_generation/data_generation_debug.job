executable = run_data_generation.sh

arguments = \
  $(Process) \
  --directory data_3 \
  --n-samples 3 \
  --n-points 5 \
  --stride 2 \
  --preruns 2 \
  --pre-N 2 \

output = logs/out.$(CLUSTER).$(PROCESS)
error  = logs/err.$(CLUSTER).$(PROCESS)
log    = logs/log.$(CLUSTER).$(PROCESS)

request_cpus = 1
request_memory = 1000
+MaxRuntime = 1000

queue 2
