executable = run_resume_training.sh

arguments = \
  --training-id 24 \
  --data-dir data_3 \
  --device cpu \
  --run-diagnostics \
  --n-diagnostic-files 5

output = logs/out.$(CLUSTER).$(PROCESS)
error  = logs/err.$(CLUSTER).$(PROCESS)
log    = logs/log.$(CLUSTER).$(PROCESS)

request_cpus = 16
request_memory = 48000

+MaxRuntime = 43200

queue
