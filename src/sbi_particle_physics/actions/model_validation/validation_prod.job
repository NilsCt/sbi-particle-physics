executable = run_validation.sh

arguments = \
  --model-dir training_12 \
  --data-dir data_3 \
  --plot-dir training_12 \
  --number-data-files 5 \
  --device cpu

output = logs/out.$(CLUSTER).$(PROCESS)
error  = logs/err.$(CLUSTER).$(PROCESS)
log    = logs/log.$(CLUSTER).$(PROCESS)

request_cpus = 2
request_memory = 8000
+MaxRuntime = 18000

queue